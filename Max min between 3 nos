#include<iostream>
#include<conio.h>

using namespace std;

int main()
{
	int a,b,c;
	cout<<"Enter any three numbers=";
	cin>>a>>b>>c;
	
	if(a>b) 
	{
		if(a>c)
        cout<<"a is greater";
		}
	if(b>a)
	{
		if(b>c)
		cout<<"b is greater";
	}
	if(c>a)
	{
		if(c>b)
		cout<<"c is greater";
		
	}
	return 0;
}
