#include<iostream>
#include<conio.h>

using namespace std;

int main()
{
	int n,i,flag=0;
	cout<<"Enter any number=";
	cin>>n;
	
	//code to check whether a given number is prim eor not
	
	for(i=2;i<=n-1;i++)
	{
		if(n%i==0)
		{
			flag=1;
			break;
		}
	}
	if(flag==0)
	cout<<"\nGiven number is prime number";
	else
	cout<<"\n Given number is not a prime number";
	return 0;
	
	
}
